    "url": "https://github.com/aguado-romina/Reacts-Child/issues"
  },
  "homepage": "https://github.com/aguado-romina/Reacts-Child#readme",
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "cloudinary": "^1.25.0",
    "decamelize": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "express": "^4.16.3",
    "find-up": "^5.0.0",
    "firebase": "^8.2.10",
    "if-env": "^1.0.4",
    "json-parse-better-errors": "^1.0.2",
    "mongoose": "^5.12.0",
