    "react-modal": "^3.12.1",
    "react-notify-toast": "^0.5.1",
    "react-router-dom": "^5.2.0",
    "set-blocking": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "nodemon": "^1.18.7"
  }
}
